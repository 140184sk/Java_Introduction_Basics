package by.introduction.level1.line;

//Вычислить значение выражения по формуле (все переменные принимают действительные значения):
//  (sin(x)+cos(y))/( cos(x) – sin(y))*tg(xy);

public class Task3 {

	public static void main(String[] args) {
		int x = 30;
		double y = 60;
		double a;
		double b;
		double c;
		double z;

		a = Math.sin(Math.toRadians(x)) + Math.cos(Math.toRadians(y));
		b = Math.cos(Math.toRadians(x)) - Math.sin(Math.toRadians(y));
		c = Math.tan(Math.toRadians(x * y));
		z = a / b * c;

		System.out.println("Значение выражения равно - " + z);

	}

}
