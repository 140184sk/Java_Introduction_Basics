package by.introduction.level1.line;

//Найдите значение функции: z = ((a-3)*b/2)+c;

public class Task1 {
	public static void main(String[] args) {
		double a;
		double b;
		double c;
		double z;
		a = 8;
		b = 3;
		c = 3;
		z = (a - 3) * b / 2 + c;
		System.out.println("Значение функции z равно - " + z);
	}
}
