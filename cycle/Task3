package by.introduction.level1.cycle;

//Найти сумму квадратов первых ста чисел.

public class Task3 {
	public static void main(String[] args) {
		long sum = 0;
		
    for (int i = 1; i <= 100; i++) {
			long sum1 = (long) Math.pow(i, 2);
			sum += sum1;
		}

		System.out.println("Сумма квадратов чисел от 1 до 100 равна:" + sum);
	}
}
