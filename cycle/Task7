package by.introduction.level1.cycle;

import java.util.Scanner;

//Для каждого натурального числа в промежутке от m до n вывести все делители, кроме единицы и самого числа.
//m и n вводятся с клавиатуры.

public class Task7 {
	public static void main(String[] args) {
		int m;
		int n;

		@SuppressWarnings("resource")
		Scanner sc = new Scanner(System.in);
		System.out.print("Введите первое натуральное число (левую границу промежутка) >>");
		m = sc.nextInt();
		System.out.print("Введите второе натуральное число (правую границу промежутка) >>");
		n = sc.nextInt();

		for (int i = m; i <= n; i++) {
			System.out.println("Дано натуральное число: " + i);
			System.out.print("Делители данного числа: ");
			for (int k = 2; k < i; k++) {
				if (i % k == 0) {
					System.out.print(k + "; ");
				} else {
				}
			}
			
      System.out.println();
			System.out.println();

		}

	}
}
